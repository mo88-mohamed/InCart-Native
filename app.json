{
  "expo": {
    "name": "InCart",
    "slug": "InCart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/in-cart-logo.png",
    "scheme": "incart",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.InCart"
    },
    "android": {
      "icon": "./assets/images/in-cart-logo.png",
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.InCart"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/in-cart-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          // "image": "./assets/images/splash-icon.png",
          "image":"./assets/images/in-cart-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dd64d2d8-5e9e-4a7f-b71b-05dcbea7e15a"
      }
    }
  }
}
